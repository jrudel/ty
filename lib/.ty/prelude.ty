export Some, None, Ok, Err

tag Some {
     map(self, f) {
          return Some(f(self));
     }

     flatMap(self, f) {
          return f(self);
     }
}

tag None {
     map()     { return None; }
     flatMap() { return None; }
}

tag Ok {
     map(self, f) {
          return Ok(f(self));
     }

     flatMap(self, f) {
          return f(self);
     }
}

tag Err {
     map(self)     { return this; }
     flatMap(self) { return this; }
}
